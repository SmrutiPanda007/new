        function getGrpTalkUserDateTime()
        {
            var sysTimeZone = -(new Date().getTimezoneOffset());
            var userTimeZone = $('#hdnOffSet').val();
            var grpTalkDate = new Date();
            grpTalkDate.setMinutes(grpTalkDate.getMinutes() + (userTimeZone - sysTimeZone))
            return grpTalkDate;
        }
		
		

