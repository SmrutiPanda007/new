        $(document).unbind('keydown').bind('keydown', function (event) {
            var doPrevent = false;
            if (event.keyCode === 8) {
                var d = event.srcElement || event.target;
                if ((d.tagName.toUpperCase() === 'INPUT' && 
					 (
						 d.type.toUpperCase() === 'TEXT' ||
						 d.type.toUpperCase() === 'PASSWORD' || 
						 d.type.toUpperCase() === 'FILE' || 
						 d.type.toUpperCase() === 'SEARCH' || 
						 d.type.toUpperCase() === 'EMAIL' || 
						 d.type.toUpperCase() === 'NUMBER' || 
						 d.type.toUpperCase() === 'DATE' )
					 ) || 
					 d.tagName.toUpperCase() === 'TEXTAREA') {
                    doPrevent = d.readOnly || d.disabled;
                }
                else {
                    doPrevent = true;
                }
            }

            if (doPrevent) {
                event.preventDefault();
            }
        });

		
        function Notifier(msg, mode) {
            var arr = msg.split('-')
            toastr.options = {
                "closeButton":false,
                "debug": true,
                "newestOnTop": true,
                "progressBar": false,
                "positionClass": "toast-top-center",
                "preventDuplicates": true,
                "timeOut": "5000",
                "extendedTimeOut": "5000",
                "showEasing": "swing",
                "hideEasing": "linear",
                "showMethod": "fadeIn",
                "hideMethod": "fadeOut"
                
            }
            if(mode==1)
            {
                toastr.success(msg, '',{onclick: function () {}});
            }
            if(mode==2)
            {
                toastr.error(msg, '',{onclick: function () {}});
            }
            
        }
		
		
		


		
		
        function CurrencyName()
        {
            var countryId=$.trim($('#hdnCountryId').val());
            var hostMobile=$.trim($('#hdnHostmobile').val());
            var qrCode=$.trim($('#hdnQrCode').val());
            console.log(qrCode);
            var currencyName = "";
            if (countryId == "240")
                currencyName = 'GBP';
            else if(countryId == "108")
               currencyName ="INR";
            else if(countryId == "241")
            {currencyName ="USD";}
            else if(countryId == "239")
            {currencyName ="AED";}
            else if(countryId == "19")
            {currencyName ="BHD";}
            return currencyName;
        }
		
        function sliceNumberFn() {
            var countryId = $('#hdnCountryId').val();
            if (countryId == 108 || countryId == 241) {
                sliceNumber = -10;
            }
            else if (countryId == 19 || countryId == 124 || countryId == 173) {
                sliceNumber = -8;
            }
            else if (countryId == 199 || countryId == 239)
            { sliceNumber = -9; }
            else
            { sliceNumber = -7; }
            return sliceNumber;
        }
		
